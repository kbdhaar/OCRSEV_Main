    <!-- http://purl.obolibrary.org/obo/MF_0000032 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MF_0000032">
        <rdfs:label xml:lang="en">bodily disposition</rdfs:label>
        <owl:equivalentClass>
